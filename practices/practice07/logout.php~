<?php
session_start();
if (isset($_SESSION["id"]) && strlen($_SESSION["id"]) > 0) {
  session_destroy();
  $_SESSION = array();
  setcookie("PHPSESSID", "", time() - 3600 * 24);
}
header("HTTP/1.1 301 Moved Permanently");
header("Location: http://cp9h7pt-anh-app000.c4sa.net/practices/practice07/login.php");
exit();
?>